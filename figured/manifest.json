{
  "manifest_version": 3,
  "name": "Figured",
  "version": "1.4.0",
  "description": "Resurrection of the old Figure It Out by TrustInsights.ai. Multiple timezones on your Chrome new tab page. Local, private, free.",
  "author": "Christopher Penn (TrustInsights.ai)",
  "homepage_url": "https://github.com/cspenn/figured",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab/newtab.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "storage",
    "alarms"
  ],
  "offline_enabled": true,
  "short_name": "Figured",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
